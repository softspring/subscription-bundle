{
    "name": "softspring/subscription-bundle",
    "description": "This bundle provides subscription features.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "softspring/admin-bundle": "^1.0",
        "softspring/account-bundle": "^1.1",
        "softspring/extra-bundle": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\SubscriptionBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "conflict": {
        "stripe/stripe-php": "<6.40.0"
    },
    "suggest": {
        "stripe/stripe-php": "Add Stripe php client library if you want to set stripe as driver"
    }
}
